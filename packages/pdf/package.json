{
  "name": "@termweb/pdf",
  "version": "0.1.0",
  "description": "Terminal-based PDF viewer powered by termweb",
  "bin": {
    "termweb-pdf": "bin/cli.js"
  },
  "scripts": {
    "build": "esbuild src/app.js --bundle --minify --outfile=dist/app.bundle.js && cp node_modules/pdfjs-dist/build/pdf.worker.min.mjs dist/pdf.worker.min.mjs",
    "start": "node bin/cli.js"
  },
  "files": [
    "bin/",
    "dist/"
  ],
  "keywords": [
    "terminal",
    "pdf",
    "viewer",
    "cli"
  ],
  "author": "teamchong",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teamchong/termweb.git",
    "directory": "packages/pdf"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "pdfjs-dist": "^4.0.379",
    "termweb": "workspace:*"
  },
  "devDependencies": {
    "esbuild": "^0.19.0"
  }
}
