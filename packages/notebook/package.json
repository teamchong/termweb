{
  "name": "@termweb/notebook",
  "version": "0.1.0",
  "description": "Terminal-based Jupyter notebook viewer/editor powered by termweb",
  "bin": {
    "termweb-notebook": "bin/cli.js"
  },
  "scripts": {
    "build": "esbuild src/app.js --bundle --minify --outfile=dist/app.bundle.js",
    "start": "node bin/cli.js"
  },
  "files": [
    "bin/",
    "dist/",
    "lib/"
  ],
  "keywords": [
    "terminal",
    "jupyter",
    "notebook",
    "ipynb",
    "cli"
  ],
  "author": "teamchong",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teamchong/termweb.git",
    "directory": "packages/notebook"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "marked": "^11.1.0",
    "termweb": "workspace:*"
  },
  "devDependencies": {
    "esbuild": "^0.19.0"
  }
}
