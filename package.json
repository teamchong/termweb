{
  "name": "termweb",
  "version": "0.8.1",
  "description": "Web browser in your terminal using Kitty graphics protocol. SDK for building terminal apps with web technologies.",
  "main": "./lib/index.js",
  "bin": {
    "termweb": "./bin/termweb.js"
  },
  "scripts": {
    "postinstall": "node scripts/install.js",
    "build:all": "bash scripts/build-all.sh"
  },
  "files": [
    "bin/",
    "lib/",
    "scripts/install.js",
    "README.md"
  ],
  "keywords": [
    "terminal",
    "browser",
    "kitty",
    "graphics",
    "cli",
    "web"
  ],
  "author": "teamch",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/teamchong/termweb"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "dependencies": {
    "ws": "^8.18.0"
  },
  "peerDependencies": {
    "ws": ">=8.0.0"
  },
  "peerDependenciesMeta": {
    "ws": {
      "optional": true
    }
  }
}
